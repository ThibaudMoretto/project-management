{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "printWidth": 100,
  "importOrder": [
    "express",
    "<THIRD_PARTY_MODULES>",
    "^(.*)config/(.*)$",
    "^(.*)datamappers/(.*)$",
    "^(.*)controllers/(.*)$",
    "^(.*)constants/(.*)$",
    "^@kelto/(.*)$",
    "middlewares",
    "utils"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}